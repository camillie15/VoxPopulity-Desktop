<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="10"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.ug.project.controller.LoginController">

    <Label text="Iniciar Sesión" style="-fx-font-size: 20;"/>
    <TextField fx:id="txtUsername" promptText="Usuario"/>
    <PasswordField fx:id="txtPassword" promptText="Contraseña"/>
    <Button text="Ingresar" onAction="#onLogin"/>

</VBox>
